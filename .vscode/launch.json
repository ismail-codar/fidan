{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "fidan-jasmine-ts",
      "program": "${workspaceFolder}/node_modules/jasmine-ts/lib/index.js",
      "args": ["${file}"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "server-examples",
      "cwd": "${workspaceFolder}/packages/fidan-examples",
      "program": "${workspaceFolder}/packages/fidan-examples/server.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "server-material",
      "cwd": "${workspaceFolder}/packages/fidan-material",
      "program": "${workspaceFolder}/packages/fidan-material/server.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "fidan-syntax",
      "runtimeArgs": ["-r", "ts-node/register"],
      "cwd": "${workspaceFolder}/packages/babel-plugin-transform-fidan-syntax",
      "args": [
        "${workspaceFolder}/packages/babel-plugin-transform-fidan-syntax/test/index.ts"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "ts-node",
      "runtimeArgs": ["-r", "ts-node/register"],
      "cwd": "${fileDirname}",
      "args": ["${file}"]
    }
  ]
}
