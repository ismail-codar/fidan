{
  "name": "@fidanjs/runtime",
  "version": "0.0.25",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "testp": "cd packages/babel-plugin-fidan-jsx && npm run testp && cd ..",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "demo": "parcel _demo/index.html --no-cache --port 1233",
    "local": "yarn build && yalc publish && yalc push"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "ismail codar",
  "module": "dist/runtime.esm.js",
  "size-limit": [
    {
      "path": "dist/runtime.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/runtime.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.1",
    "@types/react": "^17.0.3",
    "husky": "^5.1.3",
    "size-limit": "^4.10.1",
    "ts-node": "^9.1.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {}
}
